{
  "name": "immo-cool",
  "version": "3.0.0",
  "description": "immo.cool \u2014 Plateforme immobili\u00e8re suisse 100% automatis\u00e9e",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "postinstall": "DATABASE_URL=${DATABASE_URL:-postgresql://dummy:dummy@localhost:5432/dummy} prisma generate",
    "build": "DATABASE_URL=${DATABASE_URL:-postgresql://dummy:dummy@localhost:5432/dummy} prisma generate && (prisma migrate deploy 2>/dev/null || true) && next build && cp -r .next/static .next/standalone/.next/static && (cp -r public .next/standalone/public 2>/dev/null || true)",
    "start": "PORT=${PORT:-3000} HOSTNAME=0.0.0.0 node .next/standalone/server.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.460.0",
    "next": "^15.1.0",
    "next-auth": "^5.0.0-beta.30",
    "pdf-lib": "^1.17.1",
    "prisma": "^5.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.13.0",
    "stripe": "^20.3.1"
  }
}